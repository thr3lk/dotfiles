{
  "title": "calendar scroll weeks",
  "maintainers": ["justin"],
  "rules": [
    {
      "description": "Mouse Button 4 (thumb down) to Previous",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "pointing_button": "button4"
          },
          "to": [
            {
              "key_code": "left_arrow",
              "modifiers": ["left_command"]
            }
          ]
        }
      ]
    },
    {
      "description": "Mouse Button 5 (thumb up) to Next",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "pointing_button": "button5"
          },
          "to": [
            {
              "key_code": "right_arrow",
              "modifiers": ["left_command"]
            }
          ]
        }
      ]
    }
  ]
}
